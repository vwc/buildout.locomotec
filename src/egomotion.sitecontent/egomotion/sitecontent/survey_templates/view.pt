                    id="favorite1.dislikes.two"
                    name="favorite1.dislikes.two"
                    tal:attributes="value python:error and view.default_value(error) or ''" />
                  <input
                    type="text"
                    class="input-xlarge"
                    id="favorite1.dislikes.three"
                    name="favorite1.dislikes.three"
                    tal:attributes="value python:error and view.default_value(error) or ''" />
                </div>
              </div><!-- / favorite 1 dislike -->
            </div>
          </div>

        <div class="row-fluid">
            <div class="span4">
              <div class="control-group field"
                tal:define="error errors/favorite2 | nothing;
                            err_msg python:error and error['active'] == True"
                tal:attributes="class python:err_msg and 'control-group error' or 'control-group'">
